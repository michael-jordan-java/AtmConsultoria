<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context="br.com.jordan.atmconsultoria.MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/logo"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="16dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp" />

    <ImageButton
        android:id="@+id/ibEmpresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/menu_empresa"
        android:foreground="@android:color/transparent"
        android:foregroundTint="@android:color/transparent"
        android:tint="@android:color/transparent"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="146dp"
        android:layout_marginTop="28dp"
        android:layout_below="@+id/imageView"
        android:layout_alignLeft="@+id/imageView"
        android:layout_alignStart="@+id/imageView" />

    <ImageButton
        android:id="@+id/ibServico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/menu_servico"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="146dp"
        android:layout_above="@+id/ibContato"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView" />

    <ImageButton
        android:id="@+id/ibCliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/menu_cliente"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="315dp"
        android:layout_alignTop="@+id/ibContato"
        android:layout_alignLeft="@+id/ibEmpresa"
        android:layout_alignStart="@+id/ibEmpresa" />

    <ImageButton
        android:id="@+id/ibContato"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/menu_contato"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="315dp"
        android:layout_marginTop="21dp"
        android:layout_below="@+id/ibEmpresa"
        android:layout_alignLeft="@+id/ibServico"
        android:layout_alignStart="@+id/ibServico" />

</RelativeLayout>
